/********* footer **************/
#footer {
  background url(../img/cover/footer.jpg)
  background-size cover
  padding 80px 0px
  font-weight $light
  font-size 16px
  line-height 21px
  ul {
  margin-top 50px
  i.fa {
  clear left
  display block
  float left
  margin-right 20px
  margin-top 4px
  width 20px
}
 span {
  display block
  float left
  padding 0 !important
}
}
#logo-navbar-middle {
	margin-bottom 20px
}
}


@media screen and (max-width 767px) {
#footer {
	padding 0px
	ul {
    margin 50px auto
    width 300px
	float none !important
}
}
}